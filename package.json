{
  "name": "tutorstarter",
  "version": "1.1.6",
  "description": "Modern WordPress Theme for Tutor LMS",
  "main": "index.js",
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "makepot": "wp i18n make-pot --exclude='node_modules,vendor' . languages/tutorstarter.pot",
    "bundle": "dir-archiver --src . --dest ../tutorstarter.zip --exclude .DS_Store .ds_store .keep mix-manifest.json .eslintrc.json .eslintignore .git .gitattributes .github .gitignore phpcs.xml README.md composer.json composer.lock node_modules package-lock.json package.json webpack.mix.js sass reactjs src",
    "lint": "eslint \"./**/*.js\"",
    "plugin-build": "npm run production && npm run makepot && npm run bundle"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.12.10",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.26.13",
    "browser-sync-webpack-plugin": "^2.2.2",
    "cross-env": "^7.0.3",
    "dir-archiver": "^1.1.2",
    "eslint": "^7.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "jquery": "^3.5.1",
    "laravel-mix": "^5.0.9",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.30.0",
    "sass-loader": "10.1.0",
    "vue-template-compiler": "^2.6.12"
  },
  "keywords": [
    "wordpress",
    "theme",
    "tutorstarter"
  ],
  "author": "Themeum <hello@themeum.com> (https://themeum.com)",
  "license": "GPL-3.0",
  "dependencies": {
    "classnames": "^2.3.2",
    "eslint-config-wordpress": "^2.0.0",
    "normalize-scss": "^7.0.1",
    "prop-types": "^15.8.1",
    "react-visibility-sensor": "^5.1.1"
  }
}
